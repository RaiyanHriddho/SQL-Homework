CREATE TABLE emp(id integer primary key, first_name text, last_name text, salary money, commision
money);
INSERT INTO emp VALUES(1,&#39;abir&#39;, &#39;ahmed&#39;, 43324, 231);
INSERT INTO emp VALUES(2,&#39;ramesh&#39;, &#39;patwary&#39;, 63423, 0);
INSERT INTO emp VALUES(3,&#39;nadim&#39;, &#39;shah&#39;, 87765, 443);
INSERT INTO emp VALUES(4,&#39;yousuf&#39;, &#39;harun&#39;, 23424, 323);
INSERT INTO emp VALUES(5,&#39;rocky&#39;, &#39;singh&#39;, 63235, 512);
SELECT last_name,salary,commision FROM emp where commision != 0 order by salary desc,
commision desc;
